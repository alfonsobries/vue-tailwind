{
  "name": "vue-tailwind",
  "version": "0.4.0",
  "description": "Vue components styled with TailwindCss with configurable classes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alfonsobries/vue-tailwind"
  },
  "keywords": [
    "tailwindcss",
    "vue",
    "vue-tailwind",
    "vue components",
    "tailwind components"
  ],
  "author": "Alfonso Bribiesca <alfonso@vexilo.com> (https://www.vexilo.com/)",
  "license": "MIT",
  "main": "./dist/vue-tailwind.common.js",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vue-tailwind ./src/index.js",
    "lint:test": "vue-cli-service lint --no-fix",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "body-scroll-lock": "^2.6.4",
    "vue": "^2.6.10",
    "vue-popperjs": "^2.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "@vuepress/plugin-google-analytics": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "lodash": "^4.17.15",
    "tailwindcss": "^1.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.1.0",
    "webpack-dev-middleware": "^3.7.2"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "resolutions": {
    "webpack-dev-middleware": "3.6.0"
  },
  "homepage": "https://vue-tailwind.com"
}
